@model List<Menu>

@{
    ViewData["Title"] = "Home Page";
}

<!-- ======= Portfolio Section ======= -->
<section id="portfolio" class="portfolio sections-bg">
    <div class="container" data-aos="fade-up">

        <div class="section-header">
            <h2>MenuSelector</h2>
            <p>BLABLA</p>
        </div>

        <div class="portfolio-isotope" data-portfolio-filter="*" data-portfolio-layout="masonry" data-portfolio-sort="original-order" data-aos="fade-up" data-aos-delay="100">

            <div>
                <ul class="portfolio-flters">
                    <li data-filter="*" class="filter-active">Filtres</li>
                    <li data-filter=".filter-app">A</li>
                    <li data-filter=".filter-product">Refaire</li>
                    <li data-filter=".filter-branding">Voir</li>
                    <li data-filter=".filter-books">Doc</li>
                </ul><!-- End Portfolio Filters -->
            </div>

            <div class="row gy-4 portfolio-container">


                @foreach (Menu item in Model)
                {
                    <partial name="_PartialMenuBlock.cshtml"model="item"  ></partial>
                }

            </div><!-- End Portfolio Container -->

        </div>

    </div>
</section><!-- End Portfolio Section -->